{
  "name": "poe.house",
  "version": "0.0.1",
  "description": "Website for sharing Path of Exile hideouts",
  "author": "Dustin Newell",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3666"
    }
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "install": "pm2 stop api && pm2 stop poehouse && git pull origin master && npm run build && pm2 restart api && pm2 restart poehouse",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.5.3",
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/markdownit": "^1.2.2",
    "@nuxtjs/moment": "^1.1.0",
    "bootstrap": "^4.1.3",
    "cookieparser": "^0.1.0",
    "cross-env": "^5.2.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "nuxt": "^2.0.0",
    "nuxt-validate": "^0.1.2",
    "quill-image-resize-module": "^3.0.0",
    "vue-cookie-law": "^1.8.0",
    "vue-quill-editor": "^3.0.6",
    "vue-rate-it": "^2.1.0",
    "vue-recaptcha": "^1.1.1",
    "vue2-editor": "^2.6.6",
    "vuejs-paginate": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
